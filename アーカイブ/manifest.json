{
  "manifest_version": 3,
  "name": "My YouTube Dashboard",
  "version": "2.0",
  "description": "大きな画面で登録チャンネルをチェック",
  "permissions": [
    "storage",
    "history",
    "tabs"
  ],
  "host_permissions": [
    "https://www.youtube.com/"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {}
}